



entity Entry {
	
	tripTitle String,
	tripLocation String,
    tripLength Integer,
    tripDescription TextBlob,
    tripPhoto ImageBlob,
    adventure Adventure
    
}

enum Adventure {
CAMPING, HIKING

}


entity Location {

    locationName String,
	street String,
	city String,
    state String,
    zipcode String,
    latLong String
}


relationship OneToMany {
	 Location{tripLocation} to Entry
}







