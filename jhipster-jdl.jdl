



entity Entry {
	
	tripTitle String,
    tripDate LocalDate
    tripLength Integer,
    tripDescription TextBlob,
    tripPhoto ImageBlob,
    adventure Adventure,
    season Season
    
}

enum Adventure {
CAMPING, HIKING

}

enum Season {
SUMMER, FALL, SPRING, WINTER
}


entity Location {

    locationName String,
	street String,
	city String,
    state String,
    zipcode String,
    latLong String
}


relationship ManyToOne {
	 Entry{location(locationName)} to Location  
}







